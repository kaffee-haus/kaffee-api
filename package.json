{
  "name": "kaffee-api",
  "version": "0.0.0",
  "description": "Coffee Tracking API",
  "main": "dist/Index.js",
  "scripts": {
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:local": "jest --coverage",
    "build": "tsc",
    "preversion": "npm run test:local && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json,css,md}'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wel-shy/kaffee-api.git"
  },
  "author": "Daniel Welsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wel-shy/kaffee-api/issues"
  },
  "homepage": "https://github.com/wel-shy/kaffee-api#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.35",
    "coveralls": "^3.0.3",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@curveball/http-errors": "^0.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "typeorm": "^0.2.17"
  }
}
